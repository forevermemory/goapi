		func (u *UserController) List(c *gin.Context) interface{} {		log.Println("aaa")		var req = User{}		err := c.ShouldBind(&req)		if err != nil {			return config.ERROR.WithMessage(err.Error())		}			datas, err := req.List(nil)		if err != nil {			return config.ERROR.WithMessage(err.Error())		}			return config.OK.WithData(datas)	}		func (u *UserController) GetByID(c *gin.Context) interface{} {			return nil		}		func (u *UserController) Add(c *gin.Context) interface{} {			return nil		}		func (u *UserController) Update(c *gin.Context) interface{} {			return nil		}	
